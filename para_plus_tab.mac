//段落番号[000#][図#]の後に改行とタブを挿入するマクロ
//2011.8.10 作成
//***************************************************


//一括UNDO印
begingroupundo;

//段落番号[000#][図#]の後に改行とタブを挿入

replaceall "^\\[.+\\]\\f", "\\0\\n\\t", regular, nocasesense;
if( result ){
	message "段落番号[000#][図#]の後に改行とタブを挿入しました。\n";
}

//一括UNDO印
endgroupundo;

endmacro;
